name: dockercompose10892701016729606788
services:
  db-init:
    container_name: db-init
    depends_on:
      postgres:
        condition: service_healthy
        required: true
    entrypoint:
      - /bin/sh
      - /backups/db_init.sh
    environment:
      POSTGRES_DB: SmartLinkD
      POSTGRES_PASSWORD: "111"
      POSTGRES_USER: puser
    image: postgres:15.0
    networks:
      pnetwork: null
    pull_policy: missing
    restart: "no"
    volumes:
      - type: bind
        source: D:\Test\PW\SmartLinks\DB_BACKUP
        target: /backups/SmartLink.backup
        bind:
          create_host_path: true
      - type: bind
        source: D:\Test\PW\SmartLinks\Db_Init
        target: /backups/db_init.sh
        bind:
          create_host_path: true
  postgres:
    command:
      - postgres
      - -c
      - max_connections=1000
      - -c
      - shared_buffers=256MB
      - -c
      - effective_cache_size=768MB
      - -c
      - maintenance_work_mem=64MB
      - -c
      - checkpoint_completion_target=0.7
      - -c
      - wal_buffers=16MB
      - -c
      - default_statistics_target=100
      - -c
      - max_locks_per_transaction=256
    container_name: postgres_container
    deploy:
      resources:
        limits:
          cpus: 0.5
          memory: "536870912"
        reservations:
          cpus: 0.25
          memory: "268435456"
    environment:
      POSTGRES_DB: SmartLinkD
      POSTGRES_PASSWORD: "111"
      POSTGRES_USER: puser
    healthcheck:
      test:
        - CMD-SHELL
        - pg_isready -h localhost -U puser -d SmartLinkD
      timeout: 5s
      interval: 5s
      retries: 5
    image: postgres:15.0
    networks:
      pnetwork: null
    ports:
      - mode: ingress
        target: 5432
        published: "5430"
        protocol: tcp
    restart: unless-stopped
networks:
  pnetwork:
    name: dockercompose10892701016729606788_pnetwork
    driver: bridge
    ipam:
      config:
        - subnet: 172.100.0.0/16
          gateway: 172.100.0.1