name: dockercompose10892701016729606788
services:
  postgres:
    command:
      - '-'
      - sh
      - '-'
      - -c
      - '-'
      - mkdir -p /var/lib/postgresql/data && pg_restore --username=puser --host=localhost --port=5432 --dbname=SmartLinkD -d /var/lib/postgresql/data /scripts/SmartLink.backup &&  /bin/bash -c "exec pg_ctl -D /var/lib/postgresql/data start" &&  /bin/bash -c "exec pg_ctl -D /var/lib/postgresql/data status" && tail -f /var/log/postgresql/postgresql-*.log
    container_name: postgres_container
    deploy:
      resources:
        limits:
          cpus: 0.5
          memory: "536870912"
        reservations:
          cpus: 0.25
          memory: "268435456"
    environment:
      POSTGRES_DB: SmartLinkD
      POSTGRES_PASSWORD: "111"
      POSTGRES_USER: puser
    image: postgres:15.0
    networks:
      default: null
    ports:
      - mode: ingress
        target: 5432
        published: "5430"
        protocol: tcp
    restart: unless-stopped
    stdin_open: true
    tty: true
networks:
  default:
